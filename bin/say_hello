#! /app/.heroku/node/bin/node

var Parse = require('parse/node').Parse;

Parse.initialize('2cc7c323bdc48c1765d8d8d05e27d720', '51209eac0010a40227c66a9feabab99d');

function sayHello() {

    console.log('made it this far times a million');

    var query = new Parse.Query('User');
    query.find({
        success: function(users) {
            console.log(users);
            for (var i = 0; i < users.length; i++) {
                console.log(users[i].get('LastName'));
            }
        },
        error: function(err) {
            console.log(err);
        }

    })

}

sayHello();
process.exit();