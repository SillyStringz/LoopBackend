#! /app/.heroku/node/bin/node
function sayHello() {

var Parse = require('parse').Parse;

Parse.initialize("2cc7c323bdc48c1765d8d8d05e27d720", "51209eac0010a40227c66a9feabab99d");

  var query = new Parse.Query('User');
  query.find({
  success: function(data) {
  	for (var i = 0; i < data.length; i++)
  	{
  		console.log(data[i].get('LastName'));
  	}
  },
  error: function(err) {
  	console.log(err);
  }

  })

}
sayHello();
process.exit();